confirm ("Я готов играть!");
var age = prompt("Сколько тебе лет?");
if (age<13) {
    console.log ("Вы можете играть, но мы снимаем с себя вякую ответственность!");
}
else {
    console.log ("Вперед, к приключениям!");
}
console.log ("Вы находитесь на лекции по экологии");
console.log ("Внезапно Трибун кричит на всю аудиторию 'Вы хотите обоссать меня?'");
var userAnswer = prompt ("Хотите?");
if (userAnswer === "да") {
    console.log ("Вы расстегиваете ширинку и тугая струя урины звонко бьет экологу в рот");
}
else {
    console.log ("Эколог начинает плакать и заталкивать себе в очко табуретку");
}
var feedback = prompt ("Как тебе этот чудо квест?");
if (feedback >= 8) {
    console.log ("Красава!");
}
else {
    console.log ("Сперва добейся, потом критикуй!");
}
